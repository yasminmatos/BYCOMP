<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragLista"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Lista">

    <TextView
        android:id="@+id/textView16"
        android:layout_width="340dp"
        android:layout_height="45dp"
        android:layout_marginTop="108dp"
        android:text="Insira uma lista de compras e ache o mercado mais barato"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/listaProdutos"
        android:layout_width="312dp"
        android:layout_height="237dp"
        android:layout_marginTop="76dp"
        android:background="@drawable/fundo_quadrado"
        android:ems="10"
        android:inputType="textMultiLine|textPersonName"
        android:maxLines="200"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.413"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <Button
        android:id="@+id/butPesq"
        android:layout_width="122dp"
        android:layout_height="48dp"
        android:background="@drawable/botao"
        android:text="Pesquisar"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.881"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listaProdutos"
        app:layout_constraintVertical_bias="0.575" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="264dp"
        android:layout_marginBottom="32dp"
        android:text="Lista de produtos"
        app:layout_constraintBottom_toTopOf="@+id/listaProdutos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16"
        app:layout_constraintVertical_bias="1.0" />


</androidx.constraintlayout.widget.ConstraintLayout>